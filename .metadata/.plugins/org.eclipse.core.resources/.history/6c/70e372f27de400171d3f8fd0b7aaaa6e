/**
 * 
 */

/**
 * @author internousdev
 *
 */
public class person {
	public String name = null;
	public int age = 0;

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		person taro=new person();
		taro.name="山田太郎";
		taro.age=20;
		System.out.println(taro.name);
		System.out.println(taro.age);
	}
}
